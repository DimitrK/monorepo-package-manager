{
  "name": "@acme/two",
  "version": "1.0.0",
  "description": "showcasing sharing common logic across multiple yarn workspaces",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && yarn d:webpack --config webpack.config.js",
    "build:analyze": "BUNDLE_ANALYZER_ENABLED=true yarn workspace @acme/two run build",
    "clean": "rm -rf node_modules && rm -rf dist",
    "start": "webpack-dev-server --config webpack.config.js --progress --port 5101"
  },
  "author": "DimitrK",
  "license": "ISC",
  "installConfig": {
    "pnp": true
  },
  "dependencies": {
    "common": "workspace:*",
    "react": "16.6.0",
    "react-dom": "16.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "babel-loader": "8.0.6",
    "html-webpack-plugin": "3.2.0",
    "lodash": "4.17.15",
    "pnp-webpack-plugin": "^1.6.4",
    "react-hot-loader": "4.12.19",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
